{
  "name": "@enjoyjs/node-mecab",
  "version": "0.3.0",
  "description": "MeCab wrapper for Node.js",
  "type": "module",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.mjs",
    "require": "./lib/index.cjs"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "node esbuild.config.mjs",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "run-s test:*",
    "test:cjs": "node test/index.cjs",
    "test:esm": "node test/index.mjs",
    "lint": "tsc --noEmit && xo",
    "lint:fix": "tsc --noEmit && xo --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enjoyjs/node-mecab.git"
  },
  "keywords": [
    "mecab"
  ],
  "author": "Yusei Yamamoto <yuseiyamamoto.dev@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enjoyjs/node-mecab/issues"
  },
  "homepage": "https://github.com/enjoyjs/node-mecab#readme",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@enjoyjs/tsconfig": "^3.0.1",
    "@types/node": "^16.11.33",
    "@types/shell-quote": "^1.7.1",
    "@types/xo": "^0.39.2",
    "esbuild": "^0.14.38",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.0-beta",
    "xo": "^0.48.0"
  },
  "dependencies": {
    "dargs": "^7.0.0",
    "shell-quote": "^1.7.3"
  }
}
